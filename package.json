{
  "name": "parcel-boilerplate",
  "version": "0.0.4",
  "scripts": {
    "dev": "parcel source/pages/**/*.html --open",
    "build": "parcel build --dist-dir $npm_package_config_output --public-url $npm_package_config_assets source/pages/**/*.html",
    "lint": "biome lint source/scripts/*.ts",
    "lint:fix": "biome lint --write source/scripts/*.ts"
  },
  "keywords": [],
  "author": "Gabriel Jesus",
  "description": "An parcel boilerplate",
  "config": {
    "assets": ".",
    "output": "./out"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@parcel/plugin": "^2.12.0",
    "parcel": "^2.12.0",
    "postcss": "^8.4.39",
    "postcss-url": "^10.1.3",
    "posthtml-component": "^2.0.0",
    "posthtml-doctype": "^1.1.1",
    "posthtml-include": "^2.0.1",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.4"
  }
}
